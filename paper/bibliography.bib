	month = oct,
	year = {2020},
	pages = {7606},
	file = {Full Text:/Users/stanbar/Zotero/storage/2UD5TB47/Barański et al. - 2020 - Practical I-Voting on Stellar Blockchain.pdf:application/pdf},
}

@article{sobeckiPrivacyPreservingScalableBlockchainBased2022,
