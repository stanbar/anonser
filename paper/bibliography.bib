	journaltitle = {{IEEE} Access},
	author = {Wang, Shangping and Tang, Xixi and Zhang, Yaling and Chen, Juanjuan},
	date = {2019},
	note = {Conference Name: {IEEE} Access},
	keywords = {blockchain, Blockchain, Bitcoin, Companies, Smart contracts, assets audit, Logistics, Online transactions, smart contract, Transportation},
	file = {IEEE Xplore Abstract Record:/Users/stanbar/Zotero/storage/F38A5VBZ/8792156.html:text/html;IEEE Xplore Full Text PDF:/Users/stanbar/Zotero/storage/3Q3J4CLD/Wang et al. - 2019 - Auditable Protocols for Fair Payment and Physical .pdf:application/pdf},
