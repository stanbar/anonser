	date = {2020},
	note = {Conference Name: {IEEE} Access},
	keywords = {blockchain, Blockchain, traceability, Accountability, Agricultural products, credibility, reputation, Storage management, supply chain, Supply chain management, Supply chains, trust},
	file = {IEEE Xplore Abstract Record:/Users/stanbar/Zotero/storage/4DPZ8YU5/9058674.html:text/html;IEEE Xplore Full Text PDF:/Users/stanbar/Zotero/storage/KES9TN4L/Shahid et al. - 2020 - Blockchain-Based Agri-Food Supply Chain A Complet.pdf:application/pdf},
