	year = {2021},
	keywords = {Blockchains, Secure MPC, Stateless Parties, YOSO},
	pages = {64--93},
	file = {Full Text PDF:/Users/stanbar/Zotero/storage/2XMPS4CC/Gentry et al. - 2021 - YOSO You Only Speak Once.pdf:application/pdf},
}

@article{chenAlgorandSecureEfficient2019,
